{
  "expo": {
    "name": "yapper",
    "slug": "yapper-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/yapper.png",
    "scheme": "yappermobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/yapper.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/yapper.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.salehahmed99.yappermobile",
      "icon": "./assets/images/yapper.png",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["googlechrome", "itms-apps"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/yapper.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.salehahmed99.yappermobile",
      "icon": "./assets/images/yapper.png",
      "splash": {
        "image": "./assets/images/yapper.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/yapper.png",
          "backgroundColor": "#000000"
        }
      },
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "icon": "./assets/images/yapper.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/yapper.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a7bd163d-ddc7-4226-866c-747e41fcb93a"
      },
      "supportsRTL": true
    },
    "owner": "salehahmed99"
  }
}
